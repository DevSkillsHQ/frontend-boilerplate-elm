{
  "name": "frontend-boilerplate-elm",
  "version": "1.0.0",
  "scripts": {
    "build": "cd app-frontend && elm make src/Main.elm --optimize --output=main.js",
    "start": "cd app-frontend && elm-live src/Main.elm -- --output=main.js",
    "test": "cypress run"
  },
  "devDependencies": {
    "cypress": "^9.7.0",
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.7",
    "elm-live": "^4.0.2"
  }
}
